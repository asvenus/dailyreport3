<% provide(:title, 'Group users') %>
<h2> Groups Users </h2>
<div class ='center'>
<%= render 'shared/menu' %>
<div class='offset4'>
<table class='table table-striped' >
	<thead>
	<tr>
		<th>Name</th>
		<th>Email</th>
		<th></th>
	</tr>
	</thead>
	<tbody>
		<% @group_users.each do |user| %>
			<tr>
				<td><%= user.name %></td>
				<td><%= user.email %></td>
				<% if user.manager_group? %>
					<td></td>
				<% else %>
					<td><%= link_to 'Manage Users Report',"#" %></td>
				<%end%>
			</tr>								
		<% end %>
	</tbody>
</table>
</div>
<!--end div user-->
</div> <!--End div center-->
